<div class="row">
  <div class="col-xs-12">
    <ul>
      <li><%= link_to 'Add City', new_admin_city_path, class: 'btn btn-success' %></li>
    </ul>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <table class="table table_striped">
      <tr class="info">
        <td>city</td>
        <td>State Id</td>
        <td>Action</td>
      </tr>
      <tr>
        <% @cities.each do |city| %>
            <tr>
              <td><%= city.name %></td>
              <td><%= city.state_id %></td>
              <td>
                <%= link_to edit_admin_city_path(city) do %>
                    <i class="fa fa-edit"></i> edit
                <% end %>
              </td>
            </tr>
        <% end %>
      </tr>
    </table>
  </div>
</div>